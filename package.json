{
  "name": "reatransform",
  "version": "1.0.0",
  "description": "REAPER project batch transformations via scripting",
  "author": "Matt Bise <mbise1993@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mbise1993/reatransform"
  },
  "bugs": {
    "url": "https://github.com/mbise1993/reatransform/issues"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "build:server": "tsc",
    "build:client": "webpack --config ./config/webpack.config.js",
    "build": "npm run build:server & npm run build:client",
    "watch:server": "tsc -w",
    "watch:client": "webpack --watch --config ./config/webpack.config.js",
    "watch": "npm run watch:server & npm run watch:client",
    "serve": "nodemon dist/server.js",
    "dev": "npm run clean && npm run watch:server & npm run watch:client & nodemon dist/server.js",
    "start": "npm run build:server && npm run build:client && node dist/server.js",
    "test": "jest --watchAll",
    "storybook": "start-storybook",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "seed": "prisma reset -f && prisma seed"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.5",
    "file-saver": "2.0.1",
    "graphql-yoga": "^1.17.4",
    "jquery": "^3.4.0",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "jszip": "3.2.1",
    "lodash": "4.17.11",
    "prisma-client-lib": "^1.34.0",
    "react": "^16.8.6",
    "react-ace": "6.4.0",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-dom": "^16.8.6",
    "react-error-overlay": "^5.1.5",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "safe-eval": "0.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-actions": "^5.0.10",
    "@storybook/react": "^5.0.10",
    "@types/bcryptjs": "^2.4.2",
    "@types/file-saver": "^2.0.0",
    "@types/jest": "^24.0.11",
    "@types/js-cookie": "^2.2.2",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/jszip": "^3.1.5",
    "@types/lodash": "^4.14.123",
    "@types/react": "16.8.8",
    "@types/react-dom": "16.8.2",
    "@types/react-redux": "^7.1.0",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__react": "^4.0.1",
    "@types/styled-components": "4.1.14",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-preset-gatsby": "^0.1.11",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^5.0.0",
    "jest": "^24.7.1",
    "nodemon": "^1.19.1",
    "prettier": "^1.17.0",
    "react-test-renderer": "^16.8.6",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "typescript": "3.3.3",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4"
  }
}
